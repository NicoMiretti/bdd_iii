                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=17.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT 
    f.Nro_Factura,
    f.Fecha,
    rf.Id_articulo,
    rf.Unidades,
    f.Id_cliente,
    f.Id_vendedor,
    -- para Dim_Producto
    c1.Descripcion AS nombre_producto,
    c2.Descripcion AS familia_producto,
    -- para Dim_Distr_Geografica
    ci.Nom_ciudad,
    d.Nom_depto,
    -- para Dim_Vendedor
    v.Nombre AS nombre_vendedor,
    -- para Dim_Rubro
    r.Nom_rubro
FROM Facturas f
JOIN RegistrosFacturas rf ON f.Nro_Factura = rf.Nro_Factura
JOIN Articulos a ON rf.Id_articulo = a.Id_articulo
JOIN Productos p ON a.Id_producto = p.Id_producto
JOIN Codigos c1 ON c1.Tipo='Producto' AND c1.Codigo=p.Id_producto
JOIN Codigos c2 ON c2.Tipo='Familia' AND c2.Codigo=p.Id_familia
JOIN Clientes c ON f.Id_cliente = c.Id_cliente
JOIN Rubros r ON c.Id_rubro = r.Id_rubro      -- 🔹 agregado
JOIN Ciudades ci ON c.Id_ciudad = ci.Id_ciudad
JOIN Departamentos d ON c.Id_depto = d.Id_depto
JOIN Vendedores v ON f.Id_vendedor = v.Id_vendedor</property>
