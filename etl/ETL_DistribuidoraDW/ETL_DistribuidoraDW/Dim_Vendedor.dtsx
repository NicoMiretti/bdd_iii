LEFT JOIN CapacitacionXEmpleado ce ON ce.Legajo = e.Legajo
LEFT JOIN Capacitacion c ON c.Id_capacitacion = ce.Id_capacitacion
GROUP BY v.Dni, v.Nombre, e.Sueldo, e.Fecha_ingreso;</property>
